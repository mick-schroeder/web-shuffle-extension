{
   "background": {
      "page": "background.html"
   },
   "browser_action": {
      "default_icon": "icon/icon-webshuffle.png",
      "default_popup": "popup.html",
      "default_title": "Web Shuffle"
   },
   "content_scripts": [ {
      "all_frames": false,
      "js": [ "js/content_script.js", "js/blank.js"],
      "matches": [ "http://*/*", "https://*/*" ],
      "run_at": "document_idle"
   }],
   "description": "Web Shuffle",
   "icons": {
      "128": "icon/icon-webshuffle.png"
   },

   "manifest_version": 2,
   "name": "Web Shuffle",
   "options_page": "options.html",
   "permissions": [ "tabs", "http://*/*", "https://*/*", "webNavigation","storage", "notifications"],
	"version": "1.0.0",
   "homepage_url": "https://webshuffle.mickschroeder.com",   
   "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
   "web_accessible_resources": [ "blank.html" ]
}
